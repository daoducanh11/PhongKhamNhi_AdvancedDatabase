@model ICollection<PhongKhamNhi.Models.DTO.KqXnDTO>

<div class="form-group">
    <h5 style="margin: 10px 0 0 10px;">Danh sách xét nghiệm chỉ định</h5>
</div>
<table class="table table-xn">
    <thead>
        <tr>
            <th class="text-center">Tên xét nghiệm</th>
            <th class="text-center">Trị số bình thường</th>
            <th class="text-center">Đơn vị tính</th>
            <th class="text-center">Kết quả</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TenXN</td>
                <td class="text-center">@item.TriSoBinhThuong</td>
                <td class="text-center">@item.DonViTinh</td>
                <td class="text-center">@item.KetQua</td>
            </tr>
        }
    </tbody>
</table>
<div class="row" style="margin-top:10px;">
    <div class="col-md-8">

    </div>
    <div id="s-xn" class="col-md-4">
        <button class="btn btn-block btn-info btn-lg" onclick="ViewResult()">
            <i class="ace-icon fa fa-check"></i>
            Xem kết quả
        </button>
    </div>
</div>

<script>
    function ViewResult() {
        $("#phieu-xn").load("/BacSiArea/BacSiHome/ViewResult",
            {}, function (data) {

            });
    }

</script>

